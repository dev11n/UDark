/* https://docs.unity3d.com/Manual/UIE-USS-WritingStyleSheets.html */


/*****************************************/
/*	Editor Top Toolbar & Button Styling  */
/*****************************************/

/* Used for the top bar (Play, Pause and other buttons) and bottom bar */
.AppToolbar {
    border-width: 1px;
}

/****************************************************/
/*	Hierarchy, Scene, ... Toolbar & Button Styling  */
/****************************************************/

.PreToolbar,
.Toolbar {
    border-width: 1px;
}

.AnimClipToolbarButton,
.toolbarbutton {
    border-width: 0;
}

.AnimClipToolbarButton:hover,
.toolbarbutton:hover {
    background-color: #252525;
    border-top-width: 1px;
    color: #FFFFFF;
}

.AnimClipToolbarButton:checked,
.toolbarbutton:checked,
.AnimClipToolbarButton:active,
.AnimClipToolbarButton:checked:active,
.toolbarbutton:checked:active
.toolbarbutton:active {
    background-color: #202020;
    border-top-width: 1px;
    color: #FFFFFF;
}

.toolbarbuttonRight,
.toolbarbuttonLeft {
    border-right-width: 1px;
}

.toolbarbuttonRight:hover,
.toolbarbuttonLeft:hover {
    background-color: #252525;
    color: #FFFFFF;
}

.toolbarbuttonLeft:checked,
.toolbarbuttonLeft:checked:active,
.toolbarbuttonRight:active,
.toolbarbuttonRight:checked,
.toolbarbuttonRight:checked:active,
.toolbarbuttonLeft:active {
    background-color: #202020;
    color: #FFFFFF;
}

/* ToolbarPopup can mainly be seen in the Game view */
.ToolbarPopupLeft,
.ToolbarPopup {
    border-width: 0;
}

.ToolbarPopupLeft:hover,
.ToolbarPopup:hover {
    background-color: #252525;
    border-width: 0;
}

.ToolbarSeachTextField {
    border-radius: 8px;
}

.ToolbarSeachTextField:hover {
    background-color: #252525;
    color: #FFFFFF;
}

.ToolbarSearchTextFieldWithJumpPopup:hover,
.ToolbarSearchTextFieldWithJumpPopup:focus:active,
.ToolbarSeachTextField:focus:active {
    background-color: #252525;
    border-width: 1px;
}

.ToolbarSearchTextFieldWithJumpPopup {
    border-radius: 8px;
}

.ToolbarSearchTextFieldWithJumpPopup:focus:hover,
.ToolbarSearchTextFieldWithJumpPopup:focus {
    border-color: #4992ff;
    border-top-color: #4992ff;
    border-bottom-color: #4992ff;
}

